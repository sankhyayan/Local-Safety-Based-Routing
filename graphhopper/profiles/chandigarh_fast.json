{
  "priority": [
    { "if": "true", "multiply_by": 1.0 },

    { "if": "road_class == MOTORWAY", "multiply_by": 1.5 },
    { "if": "road_class == TRUNK", "multiply_by": 1.4 },
    { "if": "road_class == PRIMARY", "multiply_by": 1.3 },
    { "if": "road_class == SECONDARY", "multiply_by": 1.2 },
    { "if": "road_class == TERTIARY", "multiply_by": 1.1 },
    { "if": "road_class == RESIDENTIAL", "multiply_by": 0.6 },
    { "if": "road_class == LIVING_STREET", "multiply_by": 0.4 },
    { "if": "road_class == SERVICE", "multiply_by": 0.5 },
    { "if": "road_class == UNCLASSIFIED", "multiply_by": 0.7 },

    { "if": "max_speed > 80", "multiply_by": 1.6 },
    { "if": "max_speed > 60", "multiply_by": 1.4 },
    { "if": "max_speed > 50", "multiply_by": 1.3 },
    { "if": "max_speed < 40", "multiply_by": 0.7 },
    { "if": "max_speed < 30", "multiply_by": 0.5 },

    { "if": "road_environment == BRIDGE", "multiply_by": 1.2 },
    { "if": "road_environment == TUNNEL", "multiply_by": 1.1 },
    { "if": "surface == UNPAVED", "multiply_by": 0.6 },
    { "if": "surface == GRAVEL || surface == DIRT", "multiply_by": 0.5 },

    { "if": "in_city_center", "multiply_by": 0.9 },
    { "if": "in_market_area", "multiply_by": 0.8 },
    { "if": "in_industrial_zone", "multiply_by": 1.2 }
  ],

  "speed": [
    { "if": "true", "limit_to": 120 },

    { "if": "road_class == MOTORWAY && max_speed < 90", "limit_to": 90 },
    { "if": "road_class == TRUNK && max_speed < 80", "limit_to": 80 },
    { "if": "road_class == PRIMARY && max_speed < 70", "limit_to": 70 },
    { "if": "road_class == SECONDARY && max_speed < 60", "limit_to": 60 },
    { "if": "road_class == TERTIARY && max_speed < 55", "limit_to": 55 },
    { "if": "road_class == RESIDENTIAL", "limit_to": 40 },
    { "if": "road_class == LIVING_STREET", "limit_to": 25 },
    { "if": "road_class == SERVICE", "limit_to": 30 },
    { "if": "road_class == UNCLASSIFIED", "limit_to": 45 }
  ],

  "distance_influence": 40,

  "areas": {
    "type": "FeatureCollection",
    "features": [
      {
        "type": "Feature",
        "id": "city_center",
        "properties": { "name": "city_center" },
        "geometry": {
          "type": "Polygon",
          "coordinates": [[[76.7550, 30.7150],[76.7850, 30.7150],[76.7850, 30.7450],[76.7550, 30.7450],[76.7550, 30.7150]]]
        }
      },
      {
        "type": "Feature",
