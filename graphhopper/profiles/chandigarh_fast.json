          "coordinates": [[[76.7730, 30.7390],[76.7780, 30.7390],[76.7780, 30.7420],[76.7730, 30.7420],[76.7730, 30.7390]]]
        }
      },
      {
        "type": "Feature",
        "id": "industrial_zone",
        "properties": { "name": "industrial_zone" },
        "geometry": {
          "type": "Polygon",
          "coordinates": [[[76.7200, 30.6800],[76.7500, 30.6800],[76.7500, 30.7000],[76.7200, 30.7000],[76.7200, 30.6800]]]
        }
      }
    ]
  }
}
