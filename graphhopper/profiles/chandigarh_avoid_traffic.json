{
  "priority": [
    { "if": "true", "multiply_by": 1.0 },

    { "if": "road_class == MOTORWAY", "multiply_by": 0.8 },
    { "if": "road_class == TRUNK", "multiply_by": 0.7 },
    { "if": "road_class == PRIMARY", "multiply_by": 0.6 },
    { "if": "road_class == SECONDARY", "multiply_by": 0.9 },
    { "if": "road_class == TERTIARY", "multiply_by": 1.2 },
    { "if": "road_class == RESIDENTIAL && max_speed < 50", "multiply_by": 1.3 },
    { "if": "road_class == RESIDENTIAL && max_speed >= 50", "multiply_by": 1.0 },
    { "if": "road_class == LIVING_STREET", "multiply_by": 0.8 },
    { "if": "road_class == SERVICE", "multiply_by": 0.6 },
    { "if": "road_class == UNCLASSIFIED", "multiply_by": 1.1 },

    { "if": "in_city_center", "multiply_by": 0.5 },
    { "if": "in_market_area", "multiply_by": 0.4 },

    { "if": "max_speed > 70", "multiply_by": 0.7 },
    { "if": "max_speed < 50 && road_class != LIVING_STREET && road_class != SERVICE", "multiply_by": 1.2 },

    { "if": "road_environment == TUNNEL", "multiply_by": 0.6 },
    { "if": "road_environment == BRIDGE", "multiply_by": 0.8 },
    { "if": "surface == UNPAVED", "multiply_by": 0.9 },
    { "if": "surface == GRAVEL || surface == DIRT", "multiply_by": 0.8 }
  ],

  "speed": [
